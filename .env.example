# Core ASP.NET
ASPNETCORE_ENVIRONMENT=Production
ASPNETCORE_FORWARDEDHEADERS_ENABLED=true
ASPNETCORE_URLS=http://+:8080

# Database
DatabaseConnectionString=Host=your-host;Port=5432;Database=nucleus_db;Username=nucleus_user;Password=your_password
RedisConnectionString=your-redis-host:6379,user=default,password=your_password

# Discord OAuth (required)
DiscordClientId=
DiscordClientSecret=
DiscordBotToken=

# URLs
BackendAddress=https://your-api.example.com

# Bunny CDN (optional)
BunnyLibraryId=
BunnyAccessKey=

# Apex Legends API (optional)
ApexLegendsApiKey=

# FFmpeg
FFmpegSharedPath=/tmp/ffmpeg-downloads

# Backblaze B2 (optional - for Minecraft server backups)
Backblaze__KeyId=
Backblaze__ApplicationKey=
Backblaze__BucketName=
Backblaze__Endpoint=https://s3.eu-central-003.backblazeb2.com
Backblaze__BucketPrefix=minecraft-backups/
Backblaze__SyncIntervalHours=1
